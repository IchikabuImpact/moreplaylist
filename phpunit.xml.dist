<?xml version="1.0" encoding="UTF-8"?>
<phpunit bootstrap="vendor/autoload.php"
         colors="true"
         cacheDirectory=".phpunit.cache"
         executionOrder="random"
         failOnRisky="true"
         failOnWarning="true">
    <testsuites>
        <testsuite name="Server Side">
            <directory suffix="Test.php">tests/server</directory>
        </testsuite>
    </testsuites>
</phpunit>
